<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/manual_atte"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FCFCFC">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="0dp">

            <ImageView
                android:id="@+id/mdi_hand_ta"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/size_91"
                android:background="@drawable/hand_tap2"
                android:contentDescription="@null"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/manual_atte1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/size_12"
                android:fontFamily="@font/inter"
                android:gravity="center"
                android:text="@string/manual_atte"
                android:textAppearance="@style/manual_atte"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/mdi_hand_ta" />

            <TextView
                android:id="@+id/enter_the_s"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:fontFamily="@font/inter"
                android:gravity="center"
                android:text="@string/enter_the_s"
                android:textAppearance="@style/enter_the_s"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/manual_atte1" />

            <TextView
                android:id="@+id/student_id_"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="38dp"
                android:layout_marginTop="@dimen/size_98"
                android:fontFamily="@font/inter"
                android:gravity="center"
                android:text="@string/student_id_"
                android:textAppearance="@style/student_id_"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/enter_the_s" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/rectangle_9"
                android:layout_width="0dp"
                android:layout_height="@dimen/size_48"
                android:layout_marginHorizontal="@dimen/size_29"
                android:layout_marginTop="@dimen/size_9"
                android:background="@drawable/bg_edit_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/student_id_">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/student_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/size_12"
                    android:layout_marginVertical="@dimen/size_10"
                    android:background="@null"
                    android:drawableStart="@drawable/icon11"
                    android:imeOptions="actionDone"
                    android:drawablePadding="@dimen/size_10"
                    android:inputType="text" />
            </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:id="@+id/counter_0"
                android:layout_width="93dp"
                android:layout_height="22dp"
                android:layout_marginTop="@dimen/size_21"
                android:fontFamily="@font/inter"
                android:gravity="top"
                android:text="@string/counter_00"
                android:textAppearance="@style/counter_00"
                app:layout_constraintEnd_toEndOf="@id/rectangle_9"
                app:layout_constraintTop_toBottomOf="@id/rectangle_9" />

            <TextView
                android:id="@+id/attend"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/size_48"
                android:background="@drawable/rectangle_11"
                android:fontFamily="@font/inter"
                android:gravity="center"
                android:text="@string/attend"
                android:textAppearance="@style/attend"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/counter_0" />

            <TextView
                android:id="@+id/finish"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/size_29"
                android:background="@drawable/rectangle_222"
                android:fontFamily="@font/inter"

                android:gravity="center"
                android:text="@string/finish1"
                android:textAppearance="@style/finish1"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/attend"
                app:layout_constraintVertical_bias="0" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>